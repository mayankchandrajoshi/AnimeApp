{
  "expo": {
    "name": "AnimeApp",
    "slug": "AnimeApp",
    "version": "1.0.0",
    "scheme": "AnimeApp", 
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/splash.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
